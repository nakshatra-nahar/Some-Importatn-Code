#include <iostream>
using namespace std;

int main() {
    int num;
    cout<<"Enter A Number :- ";
    cin>>num;
    if(num==0){
        return 1;
    }        
    else{
        int count = 0;
        while(num!=0){
            num = num/10;
            count++;
        }
        cout<<count;
    }
}
